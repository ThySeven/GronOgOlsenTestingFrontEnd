            await JS.InvokeVoidAsync("pdfViewer.loadPdf", "https://raw.githubusercontent.com/ThySeven/Aflevering/main/Gr√∏nOgOlsenThySeven.pdf", "pdfCanvasContainer");
        }
    }
}